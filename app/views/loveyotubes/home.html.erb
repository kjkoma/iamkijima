<% provide(:select_link_living, "") %>
<% provide(:select_link_youtube, "_select") %>
<% provide(:select_link_instagram, "") %>

<div class="container-fluid" id="home_navi">
  <div class="col-xs-4"><a href="/"><span class="glyphicon glyphicon-home" aria-hidden="true"></span>Home</a></div>
  <div class="col-xs-4"><a href="/iam"><span class="glyphicon glyphicon-user" aria-hidden="true"></span>I am</a></div>
</div>

<div class="container-fluid">
  <div class="row-fluid">
    <div class="col-md-3 col-sm-4" id="profile_area">
      <%= render 'layouts/profile' %>
    </div>
    <div class="col-md-9 col-sm-8">
      <div class="container-fluid">
        <div class="row-fluid">
          <%= render 'layouts/header' %>
        </div>
        <div class="row-fluid">
          <div class="col-md-12" id="youtube">
            <div class="container-fluid" id="youtube_container">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script type="text/javascript" src="https://apis.google.com/js/client.js?onload=googleApiClientReady"></script>
<script type="text/javascript">
var rendered = false;
function handleAPILoaded(){
  if (!rendered) {
    search("<%= @loveyotubes.loveword %>");
    rendered = true;
  }
}
</script>
